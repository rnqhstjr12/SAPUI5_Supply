<core:FragmentDefinition 
    xmlns="sap.m" 
    xmlns:core="sap.ui.core"
    xmlns:table="sap.ui.table"
    >
    <Dialog id="ProgramProtectionDetailCancelDialog">
        <content>
            <Text textAlign="Center" class="sapUiMediumMarginBegin" text="취소하시겠습니까?&#10;(수정(작성)한 정보가 모두 사라집니다.)"/>
        </content>
        <beginButton>
            <Button text="Yes" press=".onCancelYesDialog"/>
        </beginButton>
        <endButton>
            <Button text="No" press=".onCancelNoDialog"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>