<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>
    <SelectDialog
		id="Vendor2ValueHelp"
		title="Vendor"
		items="{vendorList>/}"
		search=".onVendorValueHelpSearch"
		confirm=".onVendor2ValueHelpClose"
		cancel=".onVendor2ValueHelpClose"
	>
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{vendorList>vendorName}"
		/>
	</SelectDialog>
</c:FragmentDefinition>