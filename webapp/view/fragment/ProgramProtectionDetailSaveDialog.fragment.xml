<core:FragmentDefinition 
    xmlns="sap.m" 
    xmlns:core="sap.ui.core"
    xmlns:table="sap.ui.table"
    >
    <Dialog id="ProgramProtectionDetailSaveDialog">
        <content>
            <Text textAlign="Center" class="sapUiLargeMarginBegin" text="저장하시겠습니까?"/>
        </content>
        <beginButton>
            <Button text="Yes" press=".onSaveYesDialog"/>
        </beginButton>
        <endButton>
            <Button text="No" press=".onSaveNoDialog"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>